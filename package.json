    "nodemailer": "^6.4.11",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-component-export-image": "^0.1.6",
    "react-confetti": "^6.0.0",
    "react-dom": "^16.13.1",
    "react-dom-confetti": "^0.2.0",
    "react-modal": "^3.11.2",
    "react-router-dom": "^5.2.0",
